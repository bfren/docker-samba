{
    "$schema": "https://raw.githubusercontent.com/bfren/docker-samba/main/smb-conf-schema.json",
    "users" : [
        {
            "name": "fred",
            "pass": "password"
        },
        {
            "name": "jones",
            "pass": "another"
        }
    ],
    "shares": [
        {
            "name": "example",
            "comment": "Optional description of share",
            "users": [ "fred" ],
            "browseable": false,
            "writeable": false
        },
        {
            "name": "another",
            "users": [ "fred", "jones" ]
        }
    ]
}
